#include<stdio.h>

void main()
 {
  int n,r,i=1;
  printf("Enter Any Number : ");
  scanf("%d",&n);
  r=0;
  do
   {
    r=n%10;
    if(i==1)
     {
      printf("%d units\n",r);
      i++;
     }
    else if(i==2)
     {
      printf("%d Tens\n",r);
      i++;
     }
    else
     {
      printf("%d Hundreds",r);
     }
    n=n/10;
   }
  while(n>0);
 }
